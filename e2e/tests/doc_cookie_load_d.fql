LET url = @dynamic
LET doc = DOCUMENT(url, {
    driver: "cdp",
    cookies: [{
        name: "x-e2e",
        value: "test"
    }]
})

LET cookiesPath = LENGTH(doc.cookies) > 1 ? "ok" : "false"
LET cookie = COOKIE_GET(doc, "x-e2e")
LET expected = "ok test"

RETURN EXPECT(expected, cookiesPath + " " + cookie.value)